@model video_rental_store.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
<li>@Model.MemberShipType.Name</li>
@if (@Model.BirthDate != null)
{
    <li>@Model.BirthDate.Value.Day/@Model.BirthDate.Value.Month/@Model.BirthDate.Value.Year</li>

}

</ul>